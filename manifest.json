{
  "name": "r.get-chrome-extension",
  "description": "r.get, r.setToken, moment, lodash 제공",
  "version": "1.0.3",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "hello.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "description": "Opens hello.html"
    }
  },
  "web_accessible_resources": ["lib/lodash.js","lib/moment.min.js","lib/myCall.js"],
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
